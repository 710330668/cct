<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cv_look_round"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimenMainButtonHeight"
    android:layout_margin="@dimen/dimenToolPadding"
    app:cardBackgroundColor="@color/colorLookRoundBackground"
    app:cardCornerRadius="@dimen/dimenMainButtonRadius"
    app:cardElevation="@dimen/dimenMainButtonElevation">

    <TextView
        android:id="@+id/tv_category_name"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="@string/look_round_buy"
        android:textColor="@color/colorTitleText"
        android:textSize="@dimen/wsMainTitle" />


</android.support.v7.widget.CardView>